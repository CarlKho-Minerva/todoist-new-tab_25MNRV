{
  "manifest_version": 3,
  "name": "Todoist New Tab",
  "version": "1.0.0",
  "description": "Replace new tab page with Todoist",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html"
}
